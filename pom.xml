<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.mineacademy</groupId>
	<artifactId>Foundation</artifactId>
	<version>5.0.4</version>
	<packaging>jar</packaging>

	<name>Foundation</name>
	<description>Create complex and unique plugins easily and fast.</description>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
	</properties>

	<repositories>
		<repository>
			<id>mineacademy-repo</id>
			<url>https://bitbucket.org/mineacademy/libraries/raw/master</url>
		</repository>
		<repository>
			<id>spigot-repo</id>
			<url>https://hub.spigotmc.org/nexus/content/repositories/snapshots</url>
		</repository>
	</repositories>

	<dependencies>
		<!-- Lombok -->
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>1.18.8</version>
		</dependency>
		<!-- The Spigot API and PaperSpigot server -->
		<dependency>
			<groupId>org.spigotmc</groupId>
			<artifactId>spigot-api</artifactId>
			<version>1.14.3-R0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.server</groupId>
			<artifactId>PaperSpigot</artifactId>
			<version>1.14.3-SNAPSHOT-114</version>
		</dependency>
		<!-- Plugins from their own repo with documentation and sources -->
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>CitizensAPI</artifactId>
			<version>2.0.25-b612</version>
		</dependency>
		<!-- MineAcademy guaranteed availability repos for plugins -->
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>AuthMe</artifactId>
			<version>5.5.1-SNAPSHOT</version>
		</dependency>		
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>CMI</artifactId>
			<version>8.5.4.1</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>DiscordSRV</artifactId>
			<version>1.16.7</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>EssentialsX</artifactId>
			<version>2.16.1.80</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Factions</artifactId>
			<version>3.2.2</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>FactionsUUID</artifactId>
			<version>1.6.9.5-U0.3.0-b133</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Jobs</artifactId>
			<version>4.10.3</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>LockettePro</artifactId>
			<version>2.9.3</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>LWC</artifactId>
			<version>2.1.2</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>MassiveCore</artifactId>
			<version>3.2.2</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>mcMMO</artifactId>
			<version>1.4.06</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>MobArena</artifactId>
			<version>0.103.1</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Multiverse-Core</artifactId>
			<version>3.0.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>MVdWPlaceholderAPI</artifactId>
			<version>2.5.1</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>MythicMobs</artifactId>
			<version>4.5.6</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>NBTAPI</artifactId>
			<version>2.0.0-b66</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>NBTInjector</artifactId>
			<version>2.0.0-b66</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>NuVotifier</artifactId>
			<version>2.5.0</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>PlaceholderAPI</artifactId>
			<version>2.10.2</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>PlotSquared</artifactId>
			<version>4.222</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>ProtocolLib</artifactId>
			<version>4.5.0</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Residence</artifactId>
			<version>4.8.4.4</version>
		</dependency>		
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Towny</artifactId>
			<version>0.93.0.0</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>TownyChat</artifactId>
			<version>0.53</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>Vault</artifactId>
			<version>1.7.1</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>WorldEdit</artifactId>
			<version>7.0.0-SNAPSHOT-4486</version>
		</dependency>
		<dependency>
			<groupId>org.mineacademy.plugin</groupId>
			<artifactId>WorldGuard</artifactId>
			<version>7.0.1-SNAPSHOT-1928</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>3.1.2</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.1</version>
				<configuration>
					<source>${java.version}</source>
					<target>${java.version}</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>