<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.mineacademy.libraries</groupId>
    <artifactId>foundation</artifactId>
    <version>5.4.6-SNAPSHOT</version>
    <packaging>jar</packaging>

    <name>Foundation</name>
    <description>Create complex and unique plugins quickly and easily.</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.version>1.8</java.version>
    </properties>

    <distributionManagement>
        <repository>
            <id>mineacademy-public-libraries</id>
            <url>https://repo.mineacademy.org/repository/public-libraries/</url>
        </repository>
        <snapshotRepository>
            <id>mineacademy-public-libraries</id>
            <url>https://repo.mineacademy.org/repository/public-libraries/</url>
        </snapshotRepository>
    </distributionManagement>

    <repositories>
        <repository>
            <id>spigot-repo</id>
            <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots</url>
        </repository>
        <repository>
            <id>mineacademy-external-plugins</id>
            <url>https://repo.mineacademy.org/repository/external-plugins/</url>
        </repository>
        <repository>
            <id>jitpack.io</id>
            <url>https://jitpack.io</url>
        </repository>
        <repository>
            <id>codemc-repo</id>
            <url>https://repo.codemc.org/repository/maven-public/</url>
        </repository>
        <repository>
            <id>Exceptionflug</id>
            <url>https://mvn.exceptionflug.de/repository/exceptionflug-public/</url>
        </repository>
    </repositories>

    <dependencies>
        <!-- Lombok -->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>1.18.12</version>
            <scope>provided</scope>
        </dependency>

        <!-- The Spigot API and PaperSpigot server -->
        <dependency>
            <groupId>org.mineacademy.server</groupId>
            <artifactId>paper</artifactId>
            <version>1.16-SNAPSHOT</version>
        </dependency>

        <!-- Plugins from their own repo with documentation and sources -->
        <dependency>
            <groupId>com.github.Angeschossen</groupId>
            <artifactId>LandsAPI</artifactId>
            <version>4.5.2.0</version>
            <scope>provided</scope>
        </dependency>

        <!-- MineAcademy guaranteed availability repos for plugins -->
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>authme</artifactId>
            <version>5.6.0-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>citizens-api</artifactId>
            <version>2.0.26</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>cmi</artifactId>
            <version>8.6.20.3</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>discordsrv</artifactId>
            <version>1.19.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>essentialsx</artifactId>
            <version>2.17.2.3</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>factions</artifactId>
            <version>3.3.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>factions-uuid</artifactId>
            <version>1.6.9.5</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>jobs</artifactId>
            <version>4.15.5</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>lockette-pro</artifactId>
            <version>2.9.5</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>lwc</artifactId>
            <version>2.2.5</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>massive-core</artifactId>
            <version>3.3.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>mcmmo</artifactId>
            <version>2.1.128</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>multiverse-core</artifactId>
            <version>4.1.1-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>mvdw-placeholder-api</artifactId>
            <version>3.0.1</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>mythicmobs</artifactId>
            <version>4.7.2</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>nuvotifier</artifactId>
            <version>2.6.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>placeholder-api</artifactId>
            <version>2.10.7</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>plotsquared</artifactId>
            <version>5.4.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>protocollib</artifactId>
            <version>4.5.1-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>residence</artifactId>
            <version>4.8.8.2</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>simpleclans</artifactId>
            <version>2.12.1</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>towny</artifactId>
            <version>0.95.2.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>towny-chat</artifactId>
            <version>0.63</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>vault</artifactId>
            <version>1.7.2</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>worldedit</artifactId>
            <version>7.2.0-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mineacademy.plugins.external</groupId>
            <artifactId>worldguard</artifactId>
            <version>7.0.3-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>3.2.0</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <source>${java.version}</source>
                    <target>${java.version}</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
